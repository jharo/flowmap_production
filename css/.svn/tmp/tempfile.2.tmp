/* --------------------classes for the menu dropdown ------------------------*/
#cssmenu {
  width: auto;
  
  border: 1px solid #444;
  background: #444;
  font-size: .6em;
  
}
#cssmenu > ul {
  padding: 1px 0 1px 0;
  margin: 0px;
  list-style: none;


  height: 20px;
  /*
  border-top: 1px solid #FFFFFF;
  border-bottom: 1px solid #FFFFFF;
  */
  font-family: "Open Sans", sans-serif;
  font-size: 1.5em;
}
#cssmenu > ul li {
  margin: 0;
  padding: 0 1px 0 1px
  display: block;
  float: left;
  position: relative;
  width: 100px;
}
#cssmenu > ul li a:link,
#cssmenu > ul li a:visited {
  padding: 4px 0;
  display: block;
  text-align: center;
  text-decoration: none;
  background: #444;
  color: #ffffff;
  width: 100px;
}
#cssmenu > ul li:hover a,
#cssmenu > ul li a:hover,
#cssmenu > ul li a:active {
  padding: 4px 0;
  display: block;
  text-align: center;
  text-decoration: none;
  background: #2e82d1;
  color: #ffffff;
  width: 100px;
  border-left: 1px solid #ffffff;
  border-right: 1px solid #ffffff;
}
#cssmenu > ul li ul {
  margin: 0;
  padding: 1px 1px 0;
  list-style: none;
  display: none;
  background: #ffffff;
  width: 100px;
  position: absolute;
  top: 20px;
  left: -1px;
  border: 1px solid #444444;
  border-top: none;
}
#cssmenu > ul li:hover ul {
  display: block;
}
#cssmenu > ul li ul li {
  clear: left;
  width: 100px;
}
#cssmenu > ul li ul li a:link,
#cssmenu > ul li ul li a:visited {
  clear: left;
  background: #444444;
  padding: 4px 0;
  width: 100px;
  border: none;
  border-bottom: 1px solid #ffffff;
  position: relative;
  z-index: 1000;
}
#cssmenu > ul li ul li:hover a,
#cssmenu > ul li ul li a:active,
#cssmenu > ul li ul li a:hover {
  clear: left;
  background: #2e82d1;
  padding: 4px 0;
  width: 100px;
  border: none;
  border-bottom: 1px solid #ffffff;
  position: relative;
  z-index: 1000;
}
#cssmenu > ul li ul li ul.navigation-3 {
  display: none;
  margin: 0;
  padding: 0;
  list-style: none;
  position: absolute;
  left: 98px;
  top: -2px;
  padding: 1px 1px 0 1px;
  border: 1px solid #444444;
  border-left: 1px solid #444444;
  background: #ffffff;
  z-index: 900;
}
#cssmenu > ul li ul li:hover ul.navigation-3 {
  display: block;
}
#cssmenu > ul li ul li ul.navigation-3 li a:link,
#cssmenu > ul li ul li ul.navigation-3 li a:visited {
  background: #444444;
}

